<!DOCTYPE html>
<html lang="en">
{{> head}}
<body>
    {{> header}}
    <div class="container">
        <h1>Chapters for {{chapters.[0].course_name}}</h1>
        <ul>
            {{#each chapters}}
                <li>
                    <a href="/tests/{{this.id_hash}}">{{this.name}}</a>
                </li>
            {{/each}}
        </ul>
    </div>
    {{> footer}}
</body>
</html>
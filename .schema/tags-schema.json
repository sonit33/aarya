{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"tag_id": {
				"type": "integer",
				"minimum": 1
			},
			"tag_name": {
				"type": "string",
				"maxLength": 64
			},
			"tag_description": {
				"type": "string",
				"maxLength": 512
			}
		},
		"required": ["tag_id", "tag_name", "tag_description"],
		"additionalProperties": false
	}
}
